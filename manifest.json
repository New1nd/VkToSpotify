{
  "manifest_version": 2,
  "name": "SpotiBoom",
  "description": "Добавление треков в Spotify из VK",
  "version": "0.0.1",
  "icons": {"128": "icon_128.png"},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "script.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"]
}